} JS let total = 0;
let pedido_ = 0;

function addValue(value) {
    pedido_ += 1; 

    if (pedido_ > 2) {
        total += value * 0.10;
    } else {
        total += value;
    }
    
    document.getElementById('totalDisplay').innerText = "Total: R$ " + total.toFixed(2);
    
    mostrarcont$.innerHTML = pedido_;
}

const dcrescentar$ = document.getElementById("reducao_");
const mostrarcont$ = document.getElementById("pedido_cont");

mostrarcont$.innerHTML = pedido_;

dcrescentar$.addEventListener("click", function(){
    pedido_ = 0;
    total = 0;
    mostrarcont$.innerHTML = pedido_;
    document.getElementById('totalDisplay').innerText = "Total: R$ " + total.toFixed(2);
});
